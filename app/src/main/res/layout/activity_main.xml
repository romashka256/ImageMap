<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:ctc="http://schemas.android.com/apk/com.clickablemap.roma.clickablemap"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.clickablemap.roma.clickablemap.MainActivity">

    <com.clickablemap.roma.clickablemap.ImageMap
        android:id="@+id/map"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        ctc:map="europemap"
        ctc:maxSizeFactor="3"
        ctc:scaleFromOriginal="false"
        ctc:fitImageToScreen="false"/>

</android.support.constraint.ConstraintLayout>
